{
	"format_version": "1.8.0",
	"animations": {
		"animation.rocket_fireworks_goat.baby_scaling": {
			"loop": true,
			"bones": {
				"head": {
					"position": [0, 10, 4],
					"scale": 1.4
				}
			}
		},
		"animation.rocket_fireworks_goat.look_at_target": {
			"loop": true,
			"bones": {
				"head": {
					"relative_to": {
						"rotation": "entity"
					},
					"rotation": ["math.clamp(query.target_x_rotation, -40, 40)", "math.clamp(query.target_y_rotation, -45, 25)", 0]
				}
			}
		},
		"animation.rocket_fireworks_goat.walk": {
			"loop": true,
			"bones": {
				"right_back_leg": {
					"rotation": ["variable.tcos_right_side * 0.5", 0, 0]
				},
				"left_back_leg": {
					"rotation": ["variable.tcos_left_side * 0.5", 0, 0]
				},
				"right_front_leg": {
					"rotation": ["-variable.tcos_right_side * 0.5", 0, 0]
				},
				"left_front_leg": {
					"rotation": ["-variable.tcos_left_side * 0.5", 0, 0]
				}
			}
		},
		"animation.rocket_fireworks_goat.attack": {
			"loop": true,
			"bones": {
				"head": {
					"rotation": ["variable.attack_head_rot", 0, 0]
				}
			}
		},
		"animation.rocket_fireworks_goat.ram_attack": {
			"loop": true,
			"bones": {
				"head": {
					"rotation": ["variable.ram_head_rot", 0, 0]
				}
			}
		},
		"animation.rocket_fireworks_goat.horns": {
			"loop": true,
			"bones": {
				"right_horn": {
					"scale": "variable.goat_has_right_horn"
				},
				"left_horn": {
					"scale": "variable.goat_has_left_horn"
				}
			}
		}
	}
}